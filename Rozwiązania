#Zadanie 10.

let licznik = 5;
licznik++;
console.log(licznik);
licznik++;
console.log(licznik);
licznik--;
console.log(licznik);

#Zadanie 11.

const sekundy = 135;
let minuty = Math.floor(sekundy/60);
document.write(`${minuty}min ${sekundy%60}s`);






