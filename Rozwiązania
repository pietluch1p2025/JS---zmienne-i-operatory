#Zadanie 1.

let to jest zmienna, którą da się zmienić np.
let wiek = 19;

const to zmienna, której nie da się zmienić np.
const pi = 3.14;

const uzywamy gdy nasza zmienna ma sie nie zmieniac, a let uzywamy gdy zmienna moze sie zmienic.

#Zadanie 2.

1. Number - (liczba) - Przykład: let liczba = 41;
2. String - (tekst) - Przykład: let imie='Hania'
3. Boolean - (prawda/fałsz) - Przykład: let czyJestSlonce = false;
4. undefinded - (niezdefiniowane) - Przykład: let x;
5. null - (puste) - Przykład: let nic = null;

