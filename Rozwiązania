#Zadanie 7.

let cena = 100;
const RABAT = 0.15;
let poRabacie = (cena-cena*RABAT);
let wynik = `Cena po rabacie wynosi ${poRabacie}`;

console.log(cena);
console.log(wynik);

#Zadanie 8.

let liczba = 17;
if(liczba % 2){
  document.write('Liczba jest parzysta');
}else{
  document.write('Liczba jest nieparzysta');
}

#Zadanie 9.

